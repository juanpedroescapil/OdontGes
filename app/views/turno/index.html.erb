<p id="notice"><%= notice %></p>

<h1>Listing Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Apellido</th>
      <th>Nombre</th>
      <th>Tipo doc</th>
      <th>Num doc</th>
      <th>Fecha nac</th>
      <th>Obra soc</th>
      <th>Numero afil</th>
      <th>Grupo sang</th>
      <th>Direccion</th>
      <th>Ciudad</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Comentarios</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.apellido %></td>
        <td><%= paciente.nombre %></td>
        <td><%= paciente.tipo_doc %></td>
        <td><%= paciente.num_doc %></td>
        <td><%= paciente.fecha_nac %></td>
        <td><%= paciente.obra_soc %></td>
        <td><%= paciente.numero_afil %></td>
        <td><%= paciente.grupo_sang %></td>
        <td><%= paciente.direccion %></td>
        <td><%= paciente.ciudad %></td>
        <td><%= paciente.telefono %></td>
        <td><%= paciente.email %></td>
        <td><%= paciente.comentarios %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
